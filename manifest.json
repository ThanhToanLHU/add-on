{
    "manifest_version": 2,
    "name": "Cat Gifs!",
    "version": "1.0",
    "applications": {
        "gecko": {
            "id": "catgifs@mozilla.org"
        }
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "permissions": ["tabs", "windows"],
    "browser_action": {
        "default_title": "Cat Gdifs!"
    }
}
